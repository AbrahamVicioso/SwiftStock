generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  username String
  email    String @unique
  psw      String
  urole    String
}

model roles {
  r_id   Int    @id @default(autoincrement())
  r_name String @unique
}
